macro up G1 Z6 F200
macro printnow !self.p.send_now("M23 [0]")
macro printme
	G21
	G90
	move z 6
	home
	move z 6
	M109 {0}
	
macro loud !self.p.loud = 1
macro quiet !self.p.loud = 0

set last_file_path /home/jeanmarc/Documents/Models/things/screw cup thing 3270
set xy_feedrate 12000
set z_feedrate 200
